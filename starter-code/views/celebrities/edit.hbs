<h2>Edit Celebrity</h2>
<form method="POST" action="/celebrities-edit">
  <p> <input type="text" name="name" placeholder="{{name}}">
  </p>
  <p> <input type="text" name="occupation" placeholder="{{occupation}}">
  </p>
  <p> <input type="text" name="catchPhrase" placeholder="{{catchPhrase}}">
  </p>
  <p> <input type="submit" value="done">
  </p>
  <input type="hidden" name="_id" value="{{_id}}">
</form>